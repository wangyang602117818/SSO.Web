<?xml version="1.0" encoding="utf-8" ?>
<sql>
  <insert>
    INSERT INTO [TaskSchedulingTriggerMapping] (
    [TaskId],
    [TriggerId]
    ) VALUES (
    @TaskId,
    @TriggerId
    )
  </insert>
  <check-trigger-exists>
    select count(*) from TaskSchedulingTriggerMapping where TriggerId in (<iterate conjunction="," property="TriggerIds">@TriggerIds{{index}}</iterate>)
  </check-trigger-exists>
</sql>